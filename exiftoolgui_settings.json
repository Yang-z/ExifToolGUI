{
  "assets": {
    "ui": "exiftoolgui_mainwindow.ui",
    "no_preview": "./assets/no_preview.jpg"
  },
  "dirs": [],
  "tags_for_group": [
    "?Timeline",
    "File:FileModifyDate",
    "EXIF:DateTimeOriginal",
    "EXIF:CreateDate",
    "EXIF:ModifyDate",
    "QuickTime:CreateDate",
    "QuickTime:ModifyDate"
  ],
  "tags_for_single": {
    "all": [],
    "Datetime": [
      "SourceFile",
      "File:FileModifyDate",
      "File:FileAccessDate",
      "File:FileCreateDate",
      "?Timeline",
      "&EXIF:DateTimeOriginal",
      "EXIF:DateTimeOriginal",
      "EXIF:SubSecTimeOriginal",
      "EXIF:OffsetTimeOriginal",
      "&EXIF:CreateDate",
      "EXIF:CreateDate",
      "EXIF:SubSecTimeDigitized",
      "EXIF:OffsetTimeDigitized",
      "&EXIF:ModifyDate",
      "EXIF:ModifyDate",
      "EXIF:SubSecTime",
      "EXIF:OffsetTime",
      "EXIF:TimeZoneOffset",
      "&EXIF:GPSTimeStamp",
      "EXIF:GPSDateStamp",
      "EXIF:GPSTimeStamp",
      "&QuickTime:CreateDate",
      "QuickTime:CreateDate",
      "QuickTime:ModifyDate",
      "QuickTime:GPSDateTime"
    ],
    "GPS": [
      "SourceFile",
      "EXIF:GPSVersionID",
      "EXIF:GPSSatellites",
      "EXIF:GPSLatitude",
      "EXIF:GPSLatitudeRef",
      "EXIF:GPSLongitude",
      "EXIF:GPSLongitudeRef",
      "EXIF:GPSAltitude",
      "EXIF:GPSAltitudeRef",
      "EXIF:GPSDateStamp",
      "EXIF:GPSTimeStamp",
      "EXIF:GPSSpeed",
      "EXIF:GPSSpeedRef",
      "QuickTime:GPSSatellites",
      "QuickTime:GPSLatitude",
      "QuickTime:GPSLongitude",
      "QuickTime:GPSAltitude",
      "QuickTime:GPSDateTime",
      "QuickTime:GPSTimeStamp",
      "QuickTime:GPSSpeed",
      "QuickTime:GPSSpeedRef"
    ],
    "Custom": [
      "SourceFile"
    ]
  },
  "exiftool_options": {
    "-j": "auto",
    "-s": "auto",
    "-b": "auto",
    "-G:0:1:2:3:4:5:6:7": "forced",
    "-sep ;": "forced",
    "-n": "on",
    "-a": "on",
    "-U": "on",
    "-P": "on",
    "-overwrite_original": "on"
  },
  "exiftoolgui_options": {
    "auto_save": false,
    "max_group_level": 1,
    "simplify_group_level": true
  },
  "functions": {
    "copy_value": {
      "from_tag": {
        "type": "tag",
        "default": "File:FileModifyDate"
      },
      "to_tags": {
        "type": "tag",
        "default": "EXIF:DateTimeOriginal"
      }
    },
    "shift_datetime": {
      "tag": {
        "type": "tag",
        "default": "EXIF:DateTimeOriginal"
      },
      "to_datetime": {
        "type": "datetime",
        "default": ""
      },
      "by_timedelt": {
        "type": "timedelt",
        "default": ""
      },
      "default_timezone": {
        "type": "timezone",
        "default": ""
      }
    },
    "test_func": {
      "p1": {
        "type": "tag",
        "default": ""
      },
      "p2": {
        "type": "datetime",
        "default": ""
      },
      "p3": {
        "type": "timedelt",
        "default": ""
      },
      "p4": {
        "type": "bool",
        "default": false
      }
    }
  },
  "composite_tags": {
    "&EXIF:DateTimeOriginal": {
      "format": "(<EXIF:DateTimeOriginal>)(.<EXIF:SubSecTimeOriginal>)(<EXIF:OffsetTimeOriginal>)",
      "pattern": "(?P<EXIF:DateTimeOriginal>\\d{4}[:]?\\d{2}[:]?\\d{2}[ ]\\d{2}[:]?\\d{2}[:]?\\d{2})(?:\\.(?P<EXIF:SubSecTimeOriginal>\\d+))?(?P<EXIF:OffsetTimeOriginal>[-+]\\d{2}(?:[:]?\\d{2})?(?:[:]?\\d{2}(?:\\.\\d+)?)?)?"
    },
    "&EXIF:CreateDate": {
      "format": "(<EXIF:CreateDate>)(.<EXIF:SubSecTimeDigitized>)(<EXIF:OffsetTimeDigitized>)",
      "pattern": "(?P<EXIF:CreateDate>\\d{4}[:]?\\d{2}[:]?\\d{2}[ ]\\d{2}[:]?\\d{2}[:]?\\d{2})(?:\\.(?P<EXIF:SubSecTimeDigitized>\\d+))?(?P<EXIF:OffsetTimeDigitized>[-+]\\d{2}(?:[:]?\\d{2})?(?:[:]?\\d{2}(?:\\.\\d+)?)?)?"
    },
    "&EXIF:ModifyDate": {
      "format": "(<EXIF:ModifyDate>)(.<EXIF:SubSecTime>)(<EXIF:OffsetTime>)",
      "pattern": "(?P<EXIF:ModifyDate>\\d{4}[:]?\\d{2}[:]?\\d{2}[ ]\\d{2}[:]?\\d{2}[:]?\\d{2})(?:\\.(?P<EXIF:SubSecTime>\\d+))?(?P<EXIF:OffsetTime>[-+]\\d{2}(?:[:]?\\d{2})?(?:[:]?\\d{2}(?:\\.\\d+)?)?)?"
    },
    "&EXIF:GPSTimeStamp": {
      "format": "(<EXIF:GPSDateStamp> <EXIF:GPSTimeStamp>+00:00)",
      "pattern": "(?P<EXIF:GPSDateStamp>\\d{4}[:]?\\d{2}[:]?\\d{2})[ ](?P<EXIF:GPSTimeStamp>\\d{2}[:]?\\d{2}[:]?\\d{2}(?:\\.\\d+)?)?(?:[+-]00:00)?"
    },
    "&QuickTime:CreateDate": {
      "format": "(<QuickTime:CreateDate>+00:00)",
      "pattern": "(?P<QuickTime:CreateDate>\\d{4}[:]?\\d{2}[:]?\\d{2}[ ]\\d{2}[:]?\\d{2}[:]?\\d{2}(?:\\.\\d+)?)(?:[+-]00:00)?"
    }
  },
  "condition_tags": {
    "?Timeline": {
      "&EXIF:DateTimeOriginal": {
        "to_be_tested": "<File:FileName>",
        "pattern": "(?i).*\\.jpge?$"
      },
      "&QuickTime:CreateDate": {
        "to_be_tested": "<File:FileName>",
        "pattern": "(?i).*\\.mp4?$"
      }
    }
  }
}